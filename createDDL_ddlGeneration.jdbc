CREATE TABLE STAFF (SID INTEGER NOT NULL, DTYPE VARCHAR(31), SNAME VARCHAR(255), PRIMARY KEY (SID))
CREATE TABLE NONTEACHINGSTAFF (SID INTEGER NOT NULL, AREAEXPERTISE VARCHAR(255), PRIMARY KEY (SID))
CREATE TABLE TEACHINGSTAFF (SID INTEGER NOT NULL, QUALIFICATION VARCHAR(255), SUBJECTEXPERTISE VARCHAR(255), PRIMARY KEY (SID))
ALTER TABLE NONTEACHINGSTAFF ADD CONSTRAINT FK_NONTEACHINGSTAFF_SID FOREIGN KEY (SID) REFERENCES STAFF (SID)
ALTER TABLE TEACHINGSTAFF ADD CONSTRAINT FK_TEACHINGSTAFF_SID FOREIGN KEY (SID) REFERENCES STAFF (SID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
