CREATE TABLE STAFF (SID INTEGER NOT NULL, SNAME VARCHAR(255), PRIMARY KEY (SID))
CREATE TABLE NONTEACHINGSTAFF (SID INTEGER NOT NULL, AREAEXPERTISE VARCHAR(255), SNAME VARCHAR(255), PRIMARY KEY (SID))
CREATE TABLE TEACHINGSTAFF (SID INTEGER NOT NULL, QUALIFICATION VARCHAR(255), SNAME VARCHAR(255), SUBJECTEXPERTISE VARCHAR(255), PRIMARY KEY (SID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
